{
  "name": "ai-chat-front",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist-electron/main.js",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:web": "vite",
    "dev:electron": "vite --config vite.config.electron.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:web": "vite build",
    "build:electron": "vue-tsc --build && vite build --config vite.config.electron.ts && electron-builder",
    "build:win": "npm run build:electron -- --win",
    "build:mac": "npm run build:electron -- --mac",
    "build:linux": "npm run build:electron -- --linux",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.13.2",
    "dompurify": "^3.3.1",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "electron": "^39.1.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "sass-embedded": "^1.93.3",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  }
}
